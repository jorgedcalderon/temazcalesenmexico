<% include ../partials/header %>
<div class="container">
    <div class="row">
        <h1 style="text-align: center">Editar <%= temazcal.name %></h1>
        <div style="width: 45%; margin: 25px auto; ">
            <form action="/temazcales/<%= temazcal._id %>?_method=PUT" method="POST">
                <div class="form-group">
                    <input class="form-control" type="text" name="temazcal[name]" value="<%= temazcal.name %>">
                </div>
                <div class="form-group">
                    <input class="form-control" type="text" name="temazcal[image]" value="<%= temazcal.image %>">
                </div>
                <div class="form-group">
                    <input class="form-control" type="text" name="temazcal[description]" value="<%= temazcal.description %>">
                </div>
                <div class="form-group">
                    <button class="btn btn-lg btn-primary btn-block">Enviar</button>
                </div>
            </form>
            <a href="/temazcales">Regresar</a>
        </div>
        
        
    </div>
</div>
<% include ../partials/footer %>